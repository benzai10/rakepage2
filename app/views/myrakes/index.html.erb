<%= render 'shared/title' %>
<div class="container-fluid">
  <%= render 'myrakes/rakes_menu' %>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <!-- <h1>Productive Bookmarking</h1> -->
      <h5>
        <small>
          <%= link_to "ALL TOPICS", master_rakes_path, { class: "nav-link", id: "ga-all-bookmarking-rakes" } %>
          &nbsp;|&nbsp;
          <span class="nav-link"><strong>MY RAKES</strong></span>
        </small>
      </h5>
      <% if !flash.empty? %>
        <%= render 'shared/flash_message' %>
      <% end %>
      <div class="panel-group" id="accordion-overview">
        <%= render 'overview' %>
      </div>
    </div>
  </div>
</div>
<% if user_signed_in? %>
  <%= render 'master_rakes/new_modal' %>
  <%#= render 'myrakes/leaflet_move_modal' %>
  <%= render 'myrakes/leaflet_edit_modal2' %>
  <%= render 'myrakes/leaflet_edit_restricted_modal2' %>
  <%= render 'myrakes/leaflet_set_reminder_modal' %>
<% end %>