<div class="panel panel-default">
  <div class="panel-heading panel-overview">
    <h5 class="panel-title">
      <a data-toggle="collapse" data-parent="#accordion-overview" 
         href="#collapse-overview">
         Overview
      </a>
    </h5>
  </div>
  <div class="panel-collapse collapse in" id="collapse-overview">
    <div class="panel-body">
      <div>
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
          <li class="active"><a href="#my-rakes" role="tab" data-toggle="tab">My Rakes</a></li>
          <!-- <li><a href="#stats" data-toggle="tab">Stats</a></li> -->
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
          <div class="tab-pane active" id="my-rakes">
            <%= render 'my_rakes' %>
          </div>
          <div class="tab-pane" id="stats">
            <%#= render 'stats' %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>