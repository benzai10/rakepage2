<h1>New Channel for Master Rake <%= MasterRake.find_by_id(params[:master_rake_id]).name %></h1>
<div class="row">
  <div class="col-md-6">
    <%= bootstrap_form_for(@channel) do |f| %>
      <%= f.text_field :name %>
      <%= f.text_field :channel_type %>
      <%= f.text_field :source %>
      <%= f.hidden_field :master_rake_id, value: params[:master_rake_id] %>
      <%= f.submit "Save" %>
    <% end %>
  </div>
</div>