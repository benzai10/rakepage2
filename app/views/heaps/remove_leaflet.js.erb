if ('<%= @origin %>' == "overdue") {
  $('#due-tasks-count').html(parseInt($("#due-tasks-count").html()) - 1);
  if ($('#due-tasks-count').html() == "0") {
    $('#no-due-actions').removeClass('hidden');
  }
}
